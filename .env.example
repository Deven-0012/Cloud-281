# Smart Car Audio Surveillance Platform - Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://smart_car_user:SecurePassword123!@postgres:5432/smart_car_surveillance
POSTGRES_PASSWORD=SecurePassword123!
MONGODB_PASSWORD=SecurePassword123!
REDIS_PASSWORD=SecurePassword123!

# AWS Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-west-2

# AWS S3 Configuration
S3_BUCKET=smart-car-audio-storage-yourname

# AWS SQS Configuration
SQS_QUEUE_URL=https://sqs.us-west-2.amazonaws.com/123456789012/smart-car-audio-queue

# AWS SNS Configuration (Optional - for notifications)
SNS_TOPIC_ARN=arn:aws:sns:us-west-2:123456789012:smart-car-alerts

# ML Model Configuration
MODEL_PATH=/models/my_yamnet_human_model_fixed.keras

# Flask Configuration
FLASK_ENV=development
FLASK_APP=services/audio_ingestion_service.py

# Frontend Configuration
REACT_APP_API_URL=http://localhost:5001
REACT_APP_WS_URL=ws://localhost:5001
